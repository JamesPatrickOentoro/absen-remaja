 {\r\n        this.fetchRecommendations();\r\n    },\r\n    methods: {\r\n        async fetchRecommendations() {\r\n            try {\r\n                const response = await axios.get('absen/student/recommendations');\r\n                this.recommendations = response.data;\r\n            } catch (error) {\r\n                console.error('Error fetching data', error);\r\n            }\r\n        },\r\n        loginStudent() {\r\n            console.log(this.selectedId)\r\n            if (this.selectedId !== null) {\r\n                axios.post('absen/student/login', {id_jemaat: this.selectedId })\r\n                    .then(response => {\r\n                        // Handle the response as needed\r\n                        console.log(this.selectedId)\r\n                        console.log(response.data)\r\n                        alert(response.data.status)\r\n                    })\r\n                    .catch(error => {\r\n                        console.error('Error submitting form', error);\r\n                    });\r\n                this.selectedId = null;\r\n            }  else {\r\n                alert('not selected!')\r\n            }    \r\n        },\r\n        onItemSelected(id) {\r\n            this.selectedId = id;\r\n            console.log('update')\r\n        },\r\n        navigateToAdmin(){\r\n            this.$router.push('/admin-view')\r\n        }\r\n    },\r\n};\r\n\r\n</script>\r\n<style>\r\n/* @media (max-width: 768px) {\r\n  .content {\r\n    display: flex;\r\n    flex-direction: row;\r\n  }\r\n} */\r\n</style>"],"mappings":";AA+BA,OAAOA,kBAAiB,MAAO,qCAAoC;AACnE,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,YAAW,MAAO,+BAA+B;AAExD,eAAe;EACXC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACRF,YAAY;IAAEF;EAClB,CAAC;EACDK,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,OAAO,EAAE,EAAE;MACXC,UAAU,EAAE,IAAI;MAChBC,eAAe,EAAE,EAAE;MAAE;MACrBC,gBAAgB,EAAE,IAAI;MACtBC,YAAY,EAAE;IAClB,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,oBAAoB,CAAC,CAAC;EAC/B,CAAC;EACDC,OAAO,EAAE;IACL,