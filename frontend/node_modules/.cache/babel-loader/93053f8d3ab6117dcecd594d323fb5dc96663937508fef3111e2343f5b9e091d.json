\n            console.log(year)\r\n\r\n            axios.post('/absen/monthly-absent', {year,month})\r\n                .then(response => {\r\n                    console.log('Data fetched:', response.data);\r\n                    // Handle the fetched data as needed\r\n                })\r\n                .catch(error => {\r\n                    console.error('Error fetching monthly absent data:', error);\r\n                });\r\n        }\r\n    }\r\n};\r\n</script> -->\r\n<template>\r\n    <div>\r\n        <select v-model=\"selectedYear\">\r\n            <option v-for=\"year in years\" :key=\"year\">{{ year }}</option>\r\n        </select>\r\n        <select v-model=\"selectedMonth\">\r\n            <option v-for=\"(month, index) in months\" :key=\"month\" :value=\"index + 1\">{{ month }}</option>\r\n        </select>\r\n        <button @click=\"fetchMonthlyAbsentData\">Fetch Monthly Absent Data</button>\r\n\r\n        <div v-if=\"chartData\">\r\n            <canvas id=\"lineChart\"></canvas>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { Line } from 'vue-chartjs';\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            selectedYear: '',\r\n            selectedMonth: '',\r\n            years: ['2022', '2023', '2024'], // Add years as needed\r\n            months: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'], // Add months as needed\r\n            chartData: null // Initialize chartData as null\r\n        };\r\n    },\r\n    methods: {\r\n        fetchMonthlyAbsentData() {\r\n            const year = this.selectedYear;\r\n            const monthIndex = this.selectedMonth - 1;\r\n            const month = (monthIndex < 9 ? '0' : '') + (monthIndex + 1).toString();\r\n\r\n            axios.post('/absen/monthly-absent', { year, month })\r\n                .then(response => {\r\n                    console.log('Data fetched:', response.data);\r\n                    this.chartData = response.data; // Assign fetched data to chartData\r\n                    this.renderChart(); // Render the chart\r\n                })\r\n                .catch(error => {\r\n                    console.error('Error fetching monthly absent data:', error);\r\n                });\r\n        },\r\n        renderChart() {\r\n            if (!this.chartData) return;\r\n\r\n            const ctx = document.getElementById('lineChart').getContext('2d');\r\n\r\n            new Line(ctx, {\r\n                data: {\r\n                    labels: this.chartData.minggu,\r\n                    datasets: [{\r\n                        label: 'Jumlah',\r\n                        data: this.chartData.jumlah,\r\n                        backgroundColor: 'rgba(75, 192, 192, 0.2)',\r\n                        borderColor: 'rgba(75, 192, 192, 1)',\r\n                        borderWidth: 1\r\n                    }]\r\n                },\r\n                options: {\r\n                    responsive: true,\r\n                    maintainAspectRatio: false,\r\n                    scales: {\r\n                        xAxes: [{\r\n                            type: 'time',\r\n                            time: {\r\n                                displayFormats: {\r\n                                    week: 'MMM DD' // Format for week display\r\n                                }\r\n                            },\r\n                            scaleLabel: {\r\n                                display: true,\r\n                                labelString: 'Date'\r\n                            }\r\n                        }],\r\n                        yAxes: [{\r\n                            scaleLabel: {\r\n                                display: true,\r\n                                labelString: 'Count'\r\n                            }\r\n                        }]\r\n                    }\r\n                }\r\n            });\r\n        }\r\n    }\r\n};\r\n</script>"],"mappings":"AA6DA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,IAAG,QAAS,aAAa;AAElC,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,YAAY,EAAE,EAAE;MAChBC,aAAa,EAAE,EAAE;MACjBC,KAAK,EAAE,CAAC,MAAM,EAAE,MAAM,EAAE,MAAM,CAAC;MAAE;MACjCC,MAAM,EAAE,CAAC,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,CAAC;MAAE;MAC9FC,SAAS,EAAE,IAAG,CAAE;IACpB,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,sBAAsBA,CAAA,EAAG;MACrB,MAAMC,IAAG,GAAI,IAAI,CAACP,YAAY;MAC9B,MAAMQ,UAAS,GAAI,IAAI,CAACP,aAAY,GAAI,CAAC;MACzC,MAAMQ,KAAI,GAAI,CAACD,UAAS,GAAI,IAAI,GAAE,GAAI,EAAE,IAAI,CAACA,UAAS,GAAI,CAAC,EAAEE,QAAQ,CAAC,CAAC;MAEvEb,KAAK,CAACc,IAAI,CAAC,uBAAuB,EAAE;QAAEJ,IAAI;QAAEE;MAAM,CAAC,EAC9CG,IAAI,CAACC,QAAO,IAAK;QACdC,OAAO,CAACC,GAAG,CAAC,eAAe,EAAEF,QAAQ,CAACd,IAAI,CAAC;QAC3C,IAAI,CAACK,SAAQ,GAAIS,QAAQ,CAACd,IAAI,EAAE;QAChC,IAAI,CAACiB,WAAW,CAAC,CAAC,EAAE;MACxB,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;QACZJ,OAAO,CAACI,KAAK,CAAC,qCAAqC,EAAEA,KAAK,CAAC;MAC/D,CAAC,CAAC;IACV,CAAC;IACDF,WAAWA,CAAA,EAAG;MACV,IAAI,CAAC,IAAI,CAACZ,SAAS,EAAE;MAErB,MAAMe,GAAE,GAAIC,QAAQ,CAACC,cAAc,CAAC,WAAW,CAAC,CAACC,UAAU,CAAC,IAAI,CAAC;MAEjE,IAAIxB,IAAI,CAACqB,GAAG,EAAE;QACVpB,IAAI,EAAE;UACFwB,MAAM,EAAE,IAAI,CAACnB,SAAS,CAACoB,MAAM;UAC7BC,QAAQ,EAAE,CAAC;YACPC,KAAK,EAAE,QAAQ;YACf3B,IAAI,EAAE,IAAI,CAACK,SAAS,CAACuB,MAAM;YAC3BC,eAAe,EAAE,yBAAyB;YAC1CC,WAAW,EAAE,uBAAuB;YACpCC,WAAW,EAAE;UACjB,CAAC;QACL,CAAC;QACDC,OAAO,EAAE;UACLC,UAAU,EAAE,IAAI;UAChBC,mBAAmB,EAAE,KAAK;UAC1BC,MAAM,EAAE;YACJC,KAAK,EAAE,CAAC;cACJC,IAAI,EAAE,MAAM;cACZC,IAAI,EAAE;gBACFC,cAAc,EAAE;kBACZC,IAAI,EAAE,QAAO,CAAE;gBACnB;cACJ,CAAC;cACDC,UAAU,EAAE;gBACRC,OAAO,EAAE,IAAI;gBACbC,WAAW,EAAE;cACjB;YACJ,CAAC,CAAC;YACFC,KAAK,EAAE,CAAC;cACJH,UAAU,EAAE;gBACRC,OAAO,EAAE,IAAI;gBACbC,WAAW,EAAE;cACjB;YACJ,CAAC;UACL;QACJ;MACJ,CAAC,CAAC;IACN;EACJ;AACJ,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}