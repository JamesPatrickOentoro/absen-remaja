minLoginForm() {\r\n        this.showForm = false;\r\n        this.error = false;\r\n      },\r\n      adminLogin() {\r\n        const data = {\r\n          nama_admin: this.adminUsername,\r\n          password: this.adminPass\r\n        };\r\n        axios.post('absen/admin/login', data)\r\n          .then(response => {\r\n            if (response.data.status === 'success') {\r\n              this.$router.push('/admin-view');\r\n            } else {\r\n              console.error('Login gagal');\r\n              this.error = true;\r\n            }\r\n          })\r\n          .catch(error => {\r\n            console.error('ada error', error);\r\n          });\r\n      },\r\n      loginStudent() {\r\n        // Implement your loginStudent method logic here\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* Your existing styles for modal-overlay, modal-dialog, modal-content, modal-header, alert-danger, form-group, btn-login, and btn-close */\r\n  \r\n  /* Additional style to position the modal */\r\n  .modal-overlay {\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n    background-color: rgba(0, 0, 0, 0.5);\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n  }\r\n  </style>\r\n  "],"mappings":";AA4LE,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,YAAW,MAAO,+BAA+B;AACxD,OAAOC,QAAO,MAAO,sBAAsB,EAAE;;AAE7C,eAAe;EACbC,UAAU,EAAE;IACVF,YAAY;IACZC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,KAAK;MACfC,aAAa,EAAE,EAAE;MACjBC,SAAS,EAAE,EAAE;MACbC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,kBAAkBA,CAAA,EAAG;MACnB,IAAI,CAACL,QAAO,GAAI,IAAI;IACtB,CAAC;IACDM,mBAAmBA,CAAA,EAAG;MACpB,IAAI,CAACN,QAAO,GAAI,KAAK;MACrB,IAAI,CAACG,KAAI,GAAI,KAAK;IACpB,CAAC;IACDI,UAAUA,CAAA,EAAG;MACX,MAAMR,IAAG,GAAI;QACXS,UAAU,EAAE,IAAI,CAACP,aAAa;QAC9BQ,QAAQ,EAAE,IAAI,CAACP;MACjB,CAAC;MACDP,KAAK,CAACe,IAAI,CAAC,mBAAmB,EAAEX,IAAI,EACjCY,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIA,QAAQ,CAACb,IAAI,CA