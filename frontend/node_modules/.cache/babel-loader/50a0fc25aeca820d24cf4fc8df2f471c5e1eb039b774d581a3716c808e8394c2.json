{"ast":null,"code":"import { Line } from 'vue-chartjs';\nexport default {\n  extends: Line,\n  name: 'AdminDashboard',\n  mounted() {\n    this.renderChart({\n      labels: ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'],\n      datasets: [{\n        label: 'Monthly Absentees',\n        data: this.monthlyAbsentData,\n        backgroundColor: 'rgba(54, 162, 235, 0.2)',\n        borderColor: 'rgba(54, 162, 235, 1)',\n        borderWidth: 1\n      }]\n    }, {\n      responsive: true,\n      maintainAspectRatio: false\n    });\n  },\n  data() {\n    return {\n      monthlyAbsentData: [] // This will hold your actual data from the Flask backend\n    };\n  },\n  methods: {\n    fetchMonthlyAbsentData() {\n      const year = 2024; // Replace with your year value\n      const month = 5; // Replace with your month value\n\n      axios.post('/monthly-absent', {\n        year,\n        month\n      }).then(response => {\n        // Update the monthlyAbsentData with the fetched data\n        this.monthlyAbsentData = response.data;\n      }).catch(error => {\n        console.error('Error fetching monthly absent data:', error);\n      });\n    }\n  },\n  created() {\n    this.fetchMonthlyAbsentData(); // Fetch data when the component is created\n  }\n};","map":{"version":3,"names":["Line","extends","name","mounted","renderChart","labels","datasets","label","data","monthlyAbsentData","backgroundColor","borderColor","borderWidth","responsive","maintainAspectRatio","methods","fetchMonthlyAbsentData","year","month","axios","post","then","response","catch","error","console","created"],"sources":["D:\\New folder\\absen-remaja\\frontend\\src\\components\\AdminDashboard.vue"],"sourcesContent":["<!-- <template>\r\n    <h1>Heyy</h1>\r\n</template>\r\n\r\n\r\n<script>\r\nexport default {\r\n   \r\n    \r\n} -->\r\n<!-- </script> -->\r\n\r\n<style></style>\r\n\r\n<template>\r\n    <div>\r\n        <canvas id=\"monthl