hed:', response.data);\r\n\r\n            // Extract jumlah and minggu lists from the response data\r\n            const jumlah = response.data.jumlah;\r\n            const minggu = response.data.minggu;\r\n            \r\n            this.renderBarChart(minggu, jumlah);\r\n\r\n            // Use jumlah and minggu as needed\r\n            console.log('Jumlah:', jumlah);\r\n            console.log('Minggu:', minggu);\r\n\r\n            this.chartData = response.data; // Assign fetched data to chartData\r\n            // this.renderChart(); // Render the chart\r\n        })\r\n        .catch(error => {\r\n            console.error('Error fetching monthly absent data:', error);\r\n        });\r\n        },\r\n        renderBarChart(ming, jum) {\r\n            const ctx = document.getElementById('barChart').getContext('2d');\r\n\r\n            new Bar(ctx, {\r\n                data: {\r\n                    labels: ming,\r\n                    datasets: [{\r\n                        label: 'Jumlah',\r\n                        data: jum,\r\n                        backgroundColor: 'rgba(75, 192, 192, 0.2)',\r\n                        borderColor: 'rgba(75, 192, 192, 1)',\r\n                        borderWidth: 1\r\n                    }]\r\n                },\r\n                options: {\r\n                    responsive: true,\r\n                    maintainAspectRatio: false,\r\n                    scales: {\r\n                        xAxes: [{\r\n                            scaleLabel: {\r\n                                display: true,\r\n                                labelString: 'Minggu'\r\n                            }\r\n                        }],\r\n                        yAxes: [{\r\n                            scaleLabel: {\r\n                                display: true,\r\n                                labelString: 'Jumlah'\r\n                            }\r\n                        }]\r\n                    }\r\n                }\r\n            });\