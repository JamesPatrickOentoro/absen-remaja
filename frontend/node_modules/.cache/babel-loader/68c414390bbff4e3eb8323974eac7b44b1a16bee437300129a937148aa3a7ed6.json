amat: '',\n        foto: '',\n        status: ''\n      };\n    }\n  }\n};","map":{"version":3,"names":["axios","name","props","isVisible","type","Boolean","required","data","formData","nama","email","no_telp","gender","hobi","sekolah","tgl_lahir","temp_lahir","no_telp_ortu","kelas","daerah","kecamatan","alamat","foto","status","methods","registerStudent","res","post","console","log","closeForm","error","$router","push"],"sources":["D:\\New folder\\absen-remaja\\frontend\\src\\components\\StudentSignUp.vue"],"sourcesContent":["<template>\r\n        <div class=\"form-container\">\r\n            <h1 style=\"text-align: center;\"> Registration</h1>\r\n            <div class=\"form-group\">\r\n                <form @submit.prevent=\"registerStudent\">\r\n                    <div class=\"form-group\">\r\n                        <label for=\"nama\">Nama</label>\r\n                        <input type=\"text\" id=\"nama\" class=\"form-control\" v-model=\"formData.nama\" required>\r\n                    </div>\r\n                    <div class=\"form-group\">\r\n                        <label for=\"phone\">Nomor Telepon</label>\r\n                        <input type=\"text\" id=\"phone\" class=\"form-control\" v-model=\"formData.no_telp\" required>\r\n                    </div>\r\n                    <div class=\"form-group\">\r\n                        <label for=\"email\">Email</label>\r\n                        <input type=\"email\" id=\"email\" class=\"form-control\" v-model=\"formData.email\" required>\r\n                    </div>\r\n                    <div class=\"form-group\">\r\n                        <label for=\"hobby\">Hobi</label>\r\n                        <input type=\"text\" class=\"form-control\" v-model=\"formData.hobi\" required>\r\n                    </div>\r\n                    <div class=\"form-group\">\r\n                        <label for=\"school\">Nama Sekolah</label>\r\n                        <input type=\"text\" class=\"form-control\" v-model