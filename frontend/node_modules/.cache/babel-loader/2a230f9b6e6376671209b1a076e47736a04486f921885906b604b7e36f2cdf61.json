plate>\r\n    <div>\r\n        <h2>All Students</h2>\r\n        <table>\r\n            <thead>\r\n                <tr>\r\n                    <th>Name</th>\r\n                    <th>Date of Birth</th>\r\n                    <th>Class</th>\r\n                    <th>Phone Number</th>\r\n                    <th>Gender</th>\r\n                    <th>Actions</th>\r\n                </tr>\r\n            </thead>\r\n            <tbody>\r\n                <tr v-for=\"(student, index) in students\" :key=\"index\">\r\n                    <td>{{ student.nama }}</td>\r\n                    <td>{{ formatDate(student.tgl_lahir) }}</td>\r\n                    <td>{{ student.kelas }}</td>\r\n                    <td>{{ student.no_telp }}</td>\r\n                    <td>{{ student.gender }}</td>\r\n                    <td>\r\n                        <button @click=\"editStudent(index)\">\r\n                            <i class=\"bi bi-pencil-square\"></i> <!-- Edit icon -->\r\n                        </button>\r\n                        <button @click=\"deleteStudent(index)\">\r\n                            <i class=\"bi bi-trash\"></i><!-- Delete icon -->\r\n                        </button>\r\n                    </td>\r\n                </tr>\r\n            </tbody>\r\n        </table>\r\n\r\n        <!-- Edit Student Modal -->\r\n        <div v-if=\"showEditModal\" class=\"modal\">\r\n            <div class=\"modal-content\">\r\n                <span class=\"close\" @click=\"showEditModal = false\">&times;</span>\r\n                <h2>Edit Student</h2>\r\n                <!-- Edit form -->\r\n                <form @submit.prevent=\"submitEditForm\">\r\n                    <label for=\"edit-name\">Name:</label>\r\n                    <input type=\"text\" id=\"edit-name\" v-model=\"editedStudent.nama\" placeholder=\"Enter name\">\r\n                    <label for=\"edit-birth\">Date of Birth:</label>\r\n                    <input type=\"text\" id=\"edit-birth\" v-model=\"editedStudent.tgl_lahir\"\r\n                        placeholder=\"Enter date of birth\">\r\n                    <!-- Repeat similar structure for other fields -->\r\n                    <button type=\"submit\">Save</button>\r\n                </form>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nexport default {\r\n    name: 'StudentsList',\r\n    data() {\r\n        return {\r\n            showEditModal: false,\r\n            students: [],\r\n            editedStudent: {\r\n                nama: '',\r\n                email: '',\r\n                no_telp: '',\r\n                gender: '',\r\n                hobi: '',\r\n                sekolah: '',\r\n                tgl_lahir: '',\r\n                temp_lahir: '',\r\n                no_telp_ortu: '',\r\n                kelas: null,\r\n                daerah: '',\r\n                kecamatan: '',\r\n                alamat: '',\r\n                foto: \"nanana\",\r\n                status: \"active\"\r\n            }\r\n        };\r\n    },\r\n    created() {\r\n        this.fetchStudents();\r\n    },\r\n    methods: {\r\n        async fetchStudents() {\r\n            try {\r\n                const response = await axios.get('absen/all-students', {\r\n                    params: {\r\n                        id_jemaat: this.studentId,\r\n                        nama: this.name,\r\n                        tgl_lahir: this.birth,\r\n                        kelas: this.kelas,\r\n                        no_telp: this.no_telp,\r\n                        gender: this.gender\r\n                    }\r\n                });\r\n                this.students = response.data;\r\n            } catch (error) {\r\n                console.error('Error fetching absents:', error);\r\n            }\r\n        },\r\n        formatDate(dateString) {\r\n            const date = new Date(dateString);\r\n            const day = date.getDate().toString().padStart(2, '0');\r\n            const month = (date.getMonth() + 1).toString().padStart(2, '0');\r\n            const year = date.getFullYear();\r\n            return `${day}-${month}-${year}`;\r\n        }\r\n    }\r\n};\r\n</script>\r\n\r\n<style>\r\n/* Add your styles here */\r\n</style>"],"mappings":"AAsDA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACXC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,aAAa,EAAE,KAAK;MACpBC,QAAQ,EAAE,EAAE;MACZC,aAAa,EAAE;QACXC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE,EAAE;QACXC,MAAM,EAAE,EAAE;QACVC,IAAI,EAAE,EAAE;QACRC,OAAO,EAAE,EAAE;QACXC,SAAS,EAAE,EAAE;QACbC,UAAU,EAAE,EAAE;QACdC,YAAY,EAAE,EAAE;QAChBC,KAAK,EAAE,IAAI;QACXC,MAAM,EAAE,EAAE;QACVC,SAAS,EAAE,EAAE;QACbC,MAAM,EAAE,EAAE;QACVC,IAAI,EAAE,QAAQ;QACdC,MAAM,EAAE;MACZ;IACJ,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,aAAa,CAAC,CAAC;EACxB,CAAC;EACDC,OAAO,EAAE;IACL,MAAMD,aAAaA,CAAA,EAAG;MAClB,IAAI;QACA,MAAME,QAAO,GAAI,MAAMxB,KAAK,CAACyB,GAAG,CAAC,oBAAoB,EAAE;UACnDC,MAAM,EAAE;YACJC,SAAS,EAAE,IAAI,CAACC,SAAS;YACzBtB,IAAI,EAAE,IAAI,CAACL,IAAI;YACfW,SAAS,EAAE,IAAI,CAACiB,KAAK;YACrBd,KAAK,EAAE,IAAI,CAACA,KAAK;YACjBP,OAAO,EAAE,IAAI,CAACA,OAAO;YACrBC,MAAM,EAAE,IAAI,CAACA;UACjB;QACJ,CAAC,CAAC;QACF,IAAI,CAACL,QAAO,GAAIoB,QAAQ,CAACtB,IAAI;MACjC,EAAE,OAAO4B,KAAK,EAAE;QACZC,OAAO,CAACD,KAAK,CAAC,yBAAyB,EAAEA,KAAK,CAAC;MACnD;IACJ,CAAC;IACDE,UAAUA,CAACC,UAAU,EAAE;MACnB,MAAMC,IAAG,GAAI,IAAIC,IAAI,CAACF,UAAU,CAAC;MACjC,MAAMG,GAAE,GAAIF,IAAI,CAACG,OAAO,CAAC,CAAC,CAACC,QAAQ,CAAC,CAAC,CAACC,QAAQ,CAAC,CAAC,EAAE,GAAG,CAAC;MACtD,MAAMC,KAAI,GAAI,CAACN,IAAI,CAACO,QAAQ,CAAC,IAAI,CAAC,EAAEH,QAAQ,CAAC,CAAC,CAACC,QAAQ,CAAC,CAAC,EAAE,GAAG,CAAC;MAC/D,MAAMG,IAAG,GAAIR,IAAI,CAACS,WAAW,CAAC,CAAC;MAC/B,OAAQ,GAAEP,GAAI,IAAGI,KAAM,IAAGE,IAAK,EAAC;IACpC;EACJ;AACJ,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}